<!-- include header  -->
<%- include('include/header.ejs') %>
<!-- /include header  -->




<!-- main section start -->
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <a href="/add-user" class="border-shadow">
                <span class="text-gradient">New User <i class="fas fa-user"></i></span>
            </a>
        </div>

        <!-- form start  -->
        <form action="/" method="POST">
            <table id="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Gender</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody id="table-body">
                    <!-- include show  -->
                    <%- include('include/show.ejs')%>
                    <!-- include show  -->
                </tbody>
            </table>
        </form>
    </div>
    <!-- form end  -->
</main>
<!-- main section end-->








<!-- include footer  -->
<%- include('include/footer.ejs') %>
<!-- /include footer  -->